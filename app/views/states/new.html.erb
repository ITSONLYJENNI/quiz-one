<div class="booyah-box col-10 offset-1">
  <h1>Enter Your State!</h1>
    <br />
    <p>Tell me a little about where you've visited before!</p>
    <%= simple_form_for @states do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <br />
    <%= f.submit 'Submit', class: 'btn btn-primary' %>

  <% end %>
</div>